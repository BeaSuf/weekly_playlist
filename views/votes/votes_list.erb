<ul class="search-items">
    <%= @no_results %>
    <% @songs_data.each do |track| %>
        <li class="votes-item-container">
            <div class="img-container">
                <h3><%= track["title"] %></h3>
                <img class="search-img" src='<%= track["cover"] %>' alt="" srcset="">

                <% if !@owned_songs_ids.include? track["id"] %>
                    <% if @voted_songs_ids.include? track["id"] %>
                        <a class="link-as-btn" href='/un_vote/<%= track["id"] %>'>!!liked!!</a>
                    <% else %>
                        <a class="link-as-btn" href='/vote/<%= track["id"] %>'>like</a>
                    <% end %>
                <% end %>
            </div>
        </li>
    <% end %>
</ul>